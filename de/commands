*lampenstatus==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "statusall")"
*schalte (*) aus==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "turnoff" -light="(1)")"
*schalte (*) an==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "turnon" -light="(1)")"
*schalte (*) heller==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "brighter" -light="(1)")"
*schalte (*) dunkler|*lampe (*) dimmen|(*) dimmen|(*) dunkler==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "darker" -light="(1)")"
*neue farbe (*)==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "random" -light="(1)")"
