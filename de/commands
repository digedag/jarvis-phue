*lampenstatus==say "$(python ./plugins_installed/jarvishue/api/hue.py ${language:0:2} "statusall")"
*lampe (*) aus==say "$(python ./plugins_installed/jarvis-hue/api/hue.py ${language:0:2} "turnoff" "(1)")"
*lampe (*) an==say "$(python ./plugins_installed/jarvis-hue/api/hue.py ${language:0:2} "turnon" "(1)")"
*lampe (*) heller==say "$(python ./plugins_installed/jarvis-hue/api/hue.py ${language:0:2} "brighter" "(1)")"
*lampe (*) dunkler|*lampe (*) dimmen|(*) dimmen|(*) dunkler==say "$(python ./plugins_installed/jarvis-hue/api/hue.py ${language:0:2} "darker" "(1)")"
*farbwechsel (*)==say "$(python ./plugins_installed/jarvis-hue/api/hue.py ${language:0:2} "random" "(1)")"
