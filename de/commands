*lampenstatus==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "statusall")"
*lampe (*) aus==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "turnoff" -light="(1)")"
*lampe (*) an==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "turnon" -light="(1)")"
*lampe (*) heller==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "brighter" -light="(1)")"
*lampe (*) dunkler|*lampe (*) dimmen|(*) dimmen|(*) dunkler==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "darker" -light="(1)")"
*farbwechsel (*)==say "$(python $(pwd)/plugins_installed/jarvis-phue/classes/hue.py ${language:0:2} "random" -light="(1)")"
